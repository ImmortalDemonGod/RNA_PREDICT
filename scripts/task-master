#!/bin/bash

# Wrapper script for task-master
# This script runs the local task-master command

# Change to the project root directory
cd /Users/tomriddle1/RNA_PREDICT || exit 1

# Run the task-master command with all arguments and pipe to cat to ensure output is captured
node /Users/tomriddle1/RNA_PREDICT/node_modules/task-master-ai/bin/task-master.js "$@" | cat
